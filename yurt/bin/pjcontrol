#!/bin/bash

export PATH=/gpfs/runtime/opt/python/3.5.2/bin:$PATH
export MANPATH=/gpfs/runtime/opt/python/3.5.2/share/man:/gpfs/runtime/opt/python/3.5.2/man:$MANPATH
export CPATH=/gpfs/runtime/opt/python/3.5.2/include:$CPATH
export LIBRARY_PATH=/gpfs/runtime/opt/python/3.5.2/lib:$LIBRARY_PATH
export LD_LIBRARY_PATH=/gpfs/runtime/opt/python/3.5.2/lib:$LD_LIBRARY_PATH
export LD_RUN_PATH=/gpfs/runtime/opt/python/3.5.2/lib:$LD_RUN_PATH
export PKG_CONFIG_PATH=/gpfs/runtime/opt/python/3.5.2/lib/pkgconfig:$PKG_CONFIG_PATH

export PYTHON_DIR=/gpfs/runtime/opt/python/3.5.2

export PJCONTROLLOG=/gpfs/runtime/opt/cave-utils/yurt/log/pjcontrollog.txt
export PROJECTORDB=/gpfs/runtime/opt/cave-utils/yurt/etc/projector3.db

/gpfs/runtime/opt/cave-utils/yurt/bin/pjcontrol.py $*
